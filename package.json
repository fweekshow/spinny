{
  "name": "sidebar-agent",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rimraf node_modules && npm run clean:dbs",
    "clean:dbs": "rimraf *.db3* ||:",
    "dev": "npm run rebuild && npx --yes tsx --env-file .env --watch src/index.ts",
    "dev:dashboard": "npx --yes tsx --env-file .env src/dashboard/server.ts",
    "rebuild": "npm rebuild",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "gen:keys": "npx --yes tsx src/services/scripts/generateKeys.ts",
    "lint": "eslint .",
    "revoke": "npx --yes tsx --env-file .env src/services/scripts/revokeInstallations.ts",
    "start": "node dist/index.js",
    "start:dashboard": "node dist/dashboard/server.js",
    "typecheck": "tsc"
  },
  "resolutions": {
    "@xmtp/node-sdk": "4.0.3"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.38.19",
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@xmtp/content-type-primitives": "^2.0.2",
    "@xmtp/content-type-transaction-reference": "^2.0.2",
    "@xmtp/node-sdk": "^4.1.0",
    "better-sqlite3": "^11.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "langchain": "^0.3.31",
    "luxon": "^3.5.0",
    "pg": "^8.13.1",
    "tsx": "^4.19.2",
    "uint8arrays": "^5.1.0",
    "viem": "^2.22.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.13.0",
    "@types/pg": "^8.11.10",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "engines": {
    "node": ">=20 <24"
  }
}
